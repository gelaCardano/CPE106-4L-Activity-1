import statistics
n = int(input("Enter number of elements: "))
numbers = []
sum=0
avg=0
for i in range(n):
    x = float(input("Enter elements: "))
    numbers.append(x)
for j in numbers:
    sum = sum + j
avg = sum/n
print("The mean is: "+ str(avg))
print("The median is: "+ str(statistics.median(numbers)))
print("The mode is: "+ str(statistics.mode(numbers)))
